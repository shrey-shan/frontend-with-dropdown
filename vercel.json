{
  "version": 2,
  "name": "allionai-frontend",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_CONN_DETAILS_ENDPOINT": "/api/connection-details"
  },
  "functions": {
    "app/api/connection-details/route.ts": {
      "maxDuration": 30
    },
    "app/api/images/route.ts": {
      "maxDuration": 30
    }
  }
}
